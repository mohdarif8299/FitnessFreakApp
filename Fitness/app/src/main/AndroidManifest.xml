<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.fitnessfreak">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme"
        android:usesCleartextTraffic="true">
        <activity android:name=".exercise.ExerciseDetails"></activity>
        <activity android:name=".exercise.ExerciseActivity" />
        <activity android:name=".gymprograms.GymProgramsActivity" />
        <activity android:name=".pedometer.PedometerActivity" />
        <activity android:name=".yogasessions.YogaSessions" />
        <activity android:name=".yogasession.YogaSession" />
        <activity android:name=".nutritionplans.NutritionPlans" />
        <activity android:name=".featureprograms.FeaturedProgram" />
        <activity android:name=".HomeActivity" />
        <activity android:name=".SignupActivity" />
        <activity android:name=".LoginActivity" />
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <meta-data
            android:name="preloaded_fonts"
            android:resource="@array/preloaded_fonts" />
    </application>

</manifest>